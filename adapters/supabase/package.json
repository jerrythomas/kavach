{
  "name": "@kavach/adapter-supabase",
  "version": "1.0.0-next.26",
  "description": "Adapter to use supabase with kavach.",
  "author": "Jerry Thomas <me@jerrythomas.name>",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrythomas/kavach",
    "directory": "adapters/supabase"
  },
  "prettier": "@jerrythomas/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "tsc",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:ui": "vitest --ui",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "upgrade": "pnpm upgrade --latest && pnpm test:ci",
    "release": "pnpm publish --access public"
  },
  "devDependencies": {
    "@jerrythomas/prettier-config": "^1.0.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/ramda": "^0.29.12",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/ui": "~1.6.0",
    "eslint-config-shared": "workspace:*",
    "jsdom": "^24.1.0",
    "shared-config": "workspace:*",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vitest": "~1.6.0"
  },
  "dependencies": {
    "@kavach/core": "workspace:*",
    "@supabase/supabase-js": "^2.43.4",
    "ramda": "^0.29.1"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.js",
      "svelte": "./src/index.js"
    }
  }
}
