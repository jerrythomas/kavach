		"@unocss/preset-typography": "^0.47.5",
		"@unocss/preset-uno": "^0.44.7",
		"@unocss/reset": "^0.44.7",
		"@unocss/transformer-directives": "^0.44.7",
		"encoding": "^0.1.13",
		"eslint": "^8.28.0",
		"sass": "^1.56.2",
