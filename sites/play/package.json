		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"eslint": "^8.28.0",
		"svelte": "^3.53.1",
		"svelte-check": "^2.9.2",
		"typescript": "^4.9.3",
		"vite": "^3.2.4"
	},
	"dependencies": {
		"@kavach/adapter-supabase": "workspace:*",
		"@kavach/core": "workspace:*",
		"@kavach/svelte": "workspace:*",
		"@svelte-spice/themes": "latest",
		"@svelte-spice/form": "latest",
		"@unocss/core": "^0.44.7",
		"@unocss/preset-uno": "^0.44.7",
		"@unocss/reset": "^0.44.7",
